    "editor.formatOnSave": true,
    "tabMagnet.rules": [
        {
            "left": "$(name).c",
            "right": "$(name).h"
        },
        {
            "left": "$(name).cpp",
            "right": "$(name).h"
        },
        {
            "left": "$(name).cpp",
            "right": "$(name).hpp"
        },
        {
            "left": "*/$(name).c",
            "right": "*/$(name).h"
        },
        {
            "left": "*/$(name).cpp",
            "right": "*/$(name).h"
        },
        {
            "left": "*/$(name).cpp",
            "right": "*/$(name).hpp"
        },
        {
            "left": "$(name).c",
            "right": "include/$(name).h"
        },
        {
            "left": "$(name).cpp",
            "right": "include/$(name).h"
        },
        {
            "left": "$(name).cpp",
            "right": "include/$(name).hpp"
        },
        {
            "left": "**/$(name).cpp",
            "right": "tests/**/$(name)_tests.cpp"
        },
        {
            "left": "**/$(name).h",
            "right": "tests/**/$(name)_tests.cpp"
        }
    ]
}